APP_ENV=development
MONGO_URI=

# Used by Flask and JWT. In production set these to long random values.
SECRET_KEY=
JWT_SECRET_KEY=

# JWT expiry
JWT_ACCESS_TOKEN_EXPIRES_MINUTES=15
JWT_REFRESH_TOKEN_EXPIRES_DAYS=7

# Signed playback tokens for /video/<id>/stream
PLAYBACK_TOKEN_SALT=playback
PLAYBACK_TOKEN_EXPIRES_SECONDS=900

